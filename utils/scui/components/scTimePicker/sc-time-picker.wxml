<sc-dialog id="{{'time-picker-dialog-'+dialogCount}}" bind:open="dialogOpen" bind:close="dialogClose" bind:opened="dialogOpened" bind:closed="dialogClosed"><view class="sc-time-picker sc-class"><view class="time-show"><view class="time"><view class="hour-minute"><view class="{{!minuteView? 'active' :''}}" catch:tap="changeViewToHour">{{dateObject.hours}}</view><view>:</view><view class="{{minuteView? 'active' :''}}" catch:tap="changeViewToMinute">{{dateObject.minutesShow}}</view></view><view class="am-pm"><view class="{{AmOrPm === 'AM'?'active':''}}" catch:tap="changeClockToAM">AM</view><view class="{{AmOrPm === 'PM'?'active':''}}" catch:tap="changeClockToPM">PM</view></view></view></view><view class="time-container"><view class="time-circle"></view><view wx:if="{{!minuteView}}" class="time-hour" catch:touchmove="hourtouchmove" catch:tap="hourtap"><view class="time-item-select" style="transform: translateY(-50%) rotate({{selectTimeRotate-90}}deg);"></view><view wx:for="{{hourList}}" wx:key="hour"><view class="time-item {{'time-'+(index+1)}}"><view class="time-item-number {{ (item.hour === dateObject.hours || item.hour+12 === dateObject.hours || item.hour-12 === dateObject.hours) ? 'active': ''}}">{{item.hour}}</view></view></view></view><view wx:if="{{minuteView}}" class="time-minute" catch:touchmove="minutetouchmove" catch:tap="minutetap"><view class="time-item-select" style="transform: translateY(-50%) rotate({{selectTimeRotate-90}}deg);"><view class="time-item-number"></view></view><view wx:for="{{minuteList}}" wx:key="minute"><view class="time-item {{'time-'+(index+1)}}"><view class="time-item-number {{item.minute === dateObject.minutes ? 'active': ''}}">{{item.minute}}</view></view></view></view></view><view class="sc-time-picker-action"><sc-button sc-class="sc-btn" bind:tap="_close">取消</sc-button><sc-button sc-class="sc-btn" bind:tap="_submit">确定</sc-button></view></view></sc-dialog>