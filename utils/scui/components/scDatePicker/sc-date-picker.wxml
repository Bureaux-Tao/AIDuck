electDateObject.months === item.months && selectDateObject.years === item.years? 'selected' :''}}" wx:for="{{item}}" wx:key="{{item.date}}" data-date="{{item}}" bind:tap="_selectDate"><view><text>{{item.date}}</text></view></view></swiper-item></block></swiper></view></view></view><view class="sc-date-picker-action"><sc-button sc-class="sc-btn" bind:tap="_close">取消</sc-button><sc-button sc-class="sc-btn" bind:tap="_submit">确定</sc-button></view></view></view></sc-dialog>