<view class="sc-class"><slot></slot><form bindsubmit="_formSubmit" bindreset="_formReset" report-submit="{{reportSubmit}}"><view class="sc-hide-form-class"><button wx:if="{{showSubmit}}" class="sc-button-class form-button" capture-bind:tap="_tap" capture-bind:longpress="_longPress" capture-bind:touchend="_touchEnd" form-type="submit"><text style="font-size: inherit">{{submitText}}</text><view wx:for="{{rippleList}}" wx:key="rippleId" id="{{item.rippleId}}" style="width:{{item.width}}px;height:{{item.height}}px;left:{{item.left}}px;top:{{item.top}}px" class="sc-ripple {{item.startAnimate ? item.holdAnimate ? 'sc-ripple-animation-slow-hold' :'sc-ripple-animation-slow' : ''}}" bind:animationend="{{item.holdAnimate ? null : '_rippleAnimationEnd'}}"></view></button></view></form></view>