<view class="sc-class sc-checkbox {{reverse?'sc-checkbox-reverse':''}} {{disabled ? 'sc-checkbox-disabled' :''}} {{checked ? 'sc-checkbox-checked' : ''}}" bind:tap="{{!disabled ? '_changeCheck': ''}}"><view class="sc-checkbox-main" style="border-color: {{checked ? color : 'rgba(0,0,0,.54)'}}"><view class="sc-checkbox-icon-bg" style="background-color: {{color || '#ff4081'}};"></view><view class="sc-checkbox-icon" style=""></view><view class="sc-ripple" bind:animationend="_animationend"><view class="{{clicked && ripple? 'sc-ripple-animate' : ''}}" bind:animationend="_animationend"></view></view></view><label class="text-5">{{label}}</label></view>