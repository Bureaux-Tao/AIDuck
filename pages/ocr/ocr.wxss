.container {
    position: relative;
    padding: 20px 50rpx 20rpx;
    justify-content: flex-start;
    -webkit-justify-content: flex-start;
    background-color: #fafafa;
}

.ta {
    flex: 1;
    width: 100%;
    box-sizing: border-box;
    font-size: 20rpx;
    line-height: 60rpx;
    height: 400rpx;
}

.fun-container {
    width: 100%;
    height: 200rpx;
    background-color: #ecb1ac;
}

.fun-container-icon {
    display: flex;
    justify-content: center;
    align-items: center;
}

.icon {
    height: 80rpx;
    width: 80rpx;
    margin: 30rpx;
}

.fun-container-tips {
    display: flex;
}

.tips {
    font-size: 27rpx;
}

#aa {
    margin-left: 70rpx;
}

